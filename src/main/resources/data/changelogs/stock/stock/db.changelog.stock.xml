<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="02-10-2020-1" author="Danil Lyskin">
        <createTable tableName="stock">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="varchar(255)"/>
            <column name="duration" type="bigint"/>
            <column name="panel" type="double precision"/>
            <column name="postpayment" type="double precision"/>
            <column name="prepayment" type="double precision"/>
            <column name="start_time" type="timestamp"/>
            <column name="status" type="integer"/>
            <column name="company_id" type="bigint"/>
            <column name="poster_id" type="bigint"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
